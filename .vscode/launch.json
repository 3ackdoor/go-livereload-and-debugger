{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to remote process",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 2345,
      "host": "127.0.0.1",
      "internalConsoleOptions": "openOnSessionStart",
      "console": "internalConsole",
      "showLog": true,
      "apiVersion": 2,
      "trace": "verbose",
      // "postDebugTask": ""
    },
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "src/main.go",
    },
  ]
}